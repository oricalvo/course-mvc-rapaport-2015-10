<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 

        @Html.Grid(this.Model)

        <ul>
            @foreach (Contact contact in this.Model)
            {
                <li>
                    <span>@contact.UserName</span>
                    <a href="/Contact/Delete/@contact.ID">Delete</a>
                </li>
            }
        </ul>
    </div>
</body>
</html>
