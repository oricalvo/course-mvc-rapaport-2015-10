<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Contacts</h1>
        <ul>
            @foreach (ContactModel contact in this.Model)
            {
                <li class="@(contact.IsOldest ? "oldest" : "")">
                    <span>@contact.UserName</span>
                    <span>@contact.EMail</span>
                </li>
            }
        </ul>
    </div>
</body>
</html>
